        205, 206, 208, 209, 210, 215, 216, 216W, 225, 229, 230,
        239, 291, 301, 303, 311, 352,
   MES 160,
   MUSIC 48, 246, 246W,
   PHIL 140,
       SPAN 53, 53H, 53W, ##(not approved for PI)-put back per C. Vickery
   {HIDE-FROM-ADVICE PLAS 412}
    NonExclusive (ThisBlock)

  Label 66"Pre-Industrial Society";



########################################
##Abstract/Quantitative Reasoning
#########################################

If (Major=EECE-BA) Then

BeginSub

 1 Class (Decide=BestGrade) in MATH 119
 NonExclusive (ThisBlock)
 MinGrade 2.00
      Label 67"Math for Elementary School Teachers";

1 Group in
  (1 Class (Decide=BestGrade) in ANTH 238,
   MATH 114, 241,
   PSYCH 107,
   SOC 205
 NonExclusive (ThisBlock)
   MinGrade 2.00
 Label 68"One Statistics Course") OR
   (2 Classes in PSYCH 1073, 1071
    Label "Statistical Methods")
    Label "Statistics Course";
EndSub

Label 69"Abstract/Quantitative Reasoning for EECE"

Else
1 Group in
      (1 Class (Decide=Order) in {HIDE-FROM-ADVICE PLAS 413,}
      ANTH 238,
      {HIDE-FROM-ADVICE BASS 1211, 1211W, 1212,}
      BIOL 230,
      CSCI 12, 18, 80, 81, 82, 84, 85,
    86, 88, 95, 101, 103, 111, 141, 200:399,{Hide-From-Advice
    100, 102,}
      ECON 249,
      LCD  283,
      PHIL  109,
       {HIDE-FROM-ADVICE Phyed 165,}
      PSYCH 107,
      SOC   205, 212, 212H, 212W, 333,
      URBST 200, 201,
       {HIDE-FROM-ADVICE MATH 12, 14, 19, 21, 22, 76,}
      MATH 100, {HIDE-FROM-ADVICE 101, 102,} 111, {HIDE-FROM-ADVICE 112,}
    113, 114, 116, {HIDE-FROM-ADVICE 117, 118,} 119,
    {HIDE-FROM-ADVICE 130,} 131, 132, 135, {HIDE-FROM-ADVICE 137,}
    141, 142, 143, 151, 152, 157, 158, 200:299, 385, 385W,
    {HIDE-FROM-ADVICE 103}
    Low-Priority
       NonExclusive (ThisBlock)

  Label 70"Abstract/Quantitative Reasoning") OR
  (2 Classes in PSYCH 1073, 1071
  Label "Statistical Methods")
  Label "Abstract/Quantitative Reasoning";

Remark "Elementary/Early Childhood Education Majors Only:"
